<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="JiraTest">
	<test name="test create update and search jira issue">
		<classes>
			<class name="tests.TestCreateUpdateSearchIssue">
				<methods>
					<include name="test_User_Can_Create_Issue"/>
					<include name="test_User_Can_Update_Issue"/>
					<include name="test_User_Can_Search_Issue_By_Issue_ID"/>
				</methods>
			</class>
		</classes>
	</test>
	
	<test name="test create update and search jira issue with pagefactory">
		<classes>
			<class name="tests.TestCreateUpdateSearchIssueWithPageFactory">
				<methods>
					<include name="test_User_Can_Create_Issue"/>
					<include name="test_User_Can_Update_Issue"/>
					<include name="test_User_Can_Search_Issue_By_Issue_ID"/>
				</methods>
			</class>
		</classes>
	</test>
</suite>